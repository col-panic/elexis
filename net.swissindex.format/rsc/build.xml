<!-- Build the Web service client for Pharma and Non-Pharma using JAX-WS -->
<project default="wsimport">
	<target name="wsimport">
		<exec executable="/usr/bin/wsimport">
			<arg line="-keep -s ../src-gen -p net.swissindex.ws.pharma -extension -b ./wsdl/bindingPharma.xml ./wsdl/ws_Pharma_V101.wsdl" />
		</exec>
		<exec executable="/usr/bin/wsimport">
			<arg line="-keep -s ../src-gen -p net.swissindex.ws.nonpharma -extension -b ./wsdl/bindingNonPharma.xml ./wsdl/ws_NonPharma_V101.wsdl" />
		</exec>
	</target>
</project>
